#include <stdio.h>

int main() {
    int n;
    long long dp[31];  // Використовуємо long long для обробки великих значень

    // Зчитуємо розряди
    printf("Введіть кількість розрядів (розряди ≤ 30): ");
    scanf("%d", &n);

    // Ініціалізуємо початкові значення
    dp[1] = 2;
    dp[2] = 3;

    // Обчислюємо кількість чисел для кожного розряду
    for (int i = 3; i <= n; i++) {
        dp[i] = dp[i - 1] + dp[i - 2];
    }

    // Виводимо результат
    printf("Кількість чисел з %d розрядами: %lld\n", n, dp[n]);

    return 0;
}
